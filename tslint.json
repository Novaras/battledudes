{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": [
            true,
            {
                "singleline": "never",
                "multiline": {
                    "objects": "never",
                    "arrays": "never",
                    "functions": "never",
                    "typeLiterals": "ignore"
                }
            }
        ],
        "arrow-parens": false,
        "no-console": false,
        "max-line-length": [
            true,
            180
        ],
        "object-literal-sort-keys": false,
        "variable-name": [
            true,
            "allow-snake-case",
            "allow-leading-underscore"
        ],
        "interface-over-type-literal": false
    },
    "rulesDirectory": []
}